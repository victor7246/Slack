{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "installedESLint": true,
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "globals": [
    "require",
    "console",
    "Promise",
    "module",
    "clearInterval",
    "setInterval",
    "clearTimeout",
    "setTimeout",
    "setImmediate",
    "Map",
    "Buffer",
    "__dirname",
    "process",
    "Meteor",
    "Accounts",
    "document",
    "window"
  ],
  "rules": {
    "eqeqeq": [
      1,
      "allow-null"
    ],
    "camelcase": 1,
    "curly": [
      1,
      "all"
    ],
    "new-cap": 1,
    "no-irregular-whitespace": 1,
    "no-new": 1,
    "quotes": [
      0,
      "single"
    ],
    "no-extend-native": 1,
    "guard-for-in": 1,
    "no-use-before-define": 1,
    "no-undef": 2,
    "strict": 1,
    "no-unused-vars": 1,
    "no-console": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/react-in-jsx-scope": 2
  }
}
